<script>
	require(['/static/js/startup.js'], function (startup) {
		window.sz = {};

		sz.steamize = {};
		sz.index = {};

		sz.steamize.steamId = '<%= view.steamId %>';
		sz.steamize.page = '<%= view.page %>';
		sz.steamize.appId = '<%= view.appId %>';
